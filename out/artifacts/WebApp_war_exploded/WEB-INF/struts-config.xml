<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
        "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
    <form-beans>
        <form-bean name="RegisterBean" type="Beans.QueryBean"></form-bean>
    </form-beans>
    <action-mappings>
        <action path="/register" type = "Actions.RegisterAction" name="RegisterBean" scope = "request" >
            <forward name="Ok" path="/index.jsp" ></forward>
            <forward name="ERROR" path="/WEB-INF/result.jsp"></forward>
        </action>
        <action path="login"/>
        <action path="login"/>
    </action-mappings>
</struts-config>